{
  "manifest_version": 3,
  "name": "Job Application Recorder",
  "version": "3.2",
  "description": "Record and visualize job application details.",
  "action": {},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "scripting",
    "storage",
    "contextMenus",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  }
}